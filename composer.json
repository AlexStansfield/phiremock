{
	"keywords" : [
		"http",
		"mock",
		"server",
		"external"
	],
	"autoload" : {
		"psr-4" : {
			"Mcustiel\\Phiremock\\" : "src"
		}
	},
	"name" : "mcustiel/phiremock",
	"type" : "project",
	"description" : "A http server mocker",
	"license" : "GPL-3.0+",
	"require" : {
		"php" : ">=5.6",
		"react/http" : "dev-master",
		"mcustiel/php-simple-request" : "dev-develop",
		"mcustiel/power-route" : "dev-develop",
		"guzzlehttp/guzzle": ">=6.0,<7.0",
		"monolog/monolog" : "~1.17.2",
		"pwfisher/command-line-php" : "dev-master"
	},
	"require-dev" : {
	    "codeception/codeception" : ">=2.1",
		"fabpot/php-cs-fixer" : "~1.11.1",
		"phing/phing" : ">=2.12.0",
        "squizlabs/php_codesniffer" : ">=2.3.4",
        "phpmd/phpmd" : ">=2.3.2",
        "sebastian/phpcpd" : ">=2.0.2",
        "pdepend/pdepend" : ">=2.0.6",
        "phploc/phploc" : ">=2.1.1",
        "phpdocumentor/phpdocumentor" : ">=2.8.5"
	},
	"bin": ["bin/standalone"]
}